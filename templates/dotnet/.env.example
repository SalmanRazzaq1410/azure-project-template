# Environment
ENVIRONMENT={{ENV}}
ASPNETCORE_ENVIRONMENT={{ENV}}

# Server
ASPNETCORE_URLS=http://+:8080

# Database (uncomment and configure as needed)
# ConnectionStrings__DefaultConnection=Server=localhost;Database={{PROJECT}};User Id=user;Password=password;

# Redis (uncomment and configure as needed)
# REDIS_URL=localhost:6379

# Azure (populated by deployment)
# APPLICATIONINSIGHTS_CONNECTION_STRING=
# KEY_VAULT_URI=
# STORAGE_CONNECTION_STRING=

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Logging
Logging__LogLevel__Default=Information
